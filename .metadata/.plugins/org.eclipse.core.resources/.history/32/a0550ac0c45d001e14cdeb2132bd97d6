	public static By subject=By.xpath("//input[@name='subject']");
	public static By attachementFilePath=By.xpath("//input[@name='attachment']");
	public static By contentType=By.xpath("//div[@id='contenttype-bodyEl']");
	public static By contentTypeList=By.xpath("//div[contains(@class,'x-boundlist-floating') and not(contains(@style,'display: none;'))]//div[@class='list-ct x-body-masked']/ul/li");
	public static By TLVPEID=By.xpath("//input[@name='dltPEId']");
	public static By TLVTemplateID=By.xpath("//input[@name='dltTemplateId']");
	public static By TLVTelemarketerID=By.xpath("//input[@name='dltTelemarketerId']");
	public static By message=By.xpath("//textarea[@name='message']");
	public static By messageEmail=By.xpath("//body[not(contains(@id,'ext-gen'))]");
