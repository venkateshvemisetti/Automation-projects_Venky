	public static By openEditor=By.xpath("//div/label[@id='msgID']/a");
	public static By saveEmailMessage=By.xpath("//button[not(contains(@id,'saveOrUpdate'))]/span[.='Save']");
	public static By saveBTN=By.xpath("//span[@id='saveOrUpdatebottomID-btnInnerEl']");
	public static By updateBTN=By.xpath("//span[@id='saveOrUpdatebottomID-btnInnerEl']");
	public static By cancelBTN=By.xpath("//div[@id='backbutton-bottom']//span[contains(text(),'Cancel')]");
	public static By alerActiontOK=By.xpath("//button/span[contains(@id,'btnInnerEl') and contains(.,'OK')]");
	public static By searchfiled=By.xpath("//div[@id='searchField-bodyEl']/input");
	public static By disableAction=By.xpath("//tr/td[@title='Disable Action']");
	public static By yesBTN=By.xpath("//button/span[contains(@id,'btnInnerEl') and contains(.,'Yes')]");
	public static By enableAction=By.xpath("//tr/td[@title='Enable Action']");
	public static By searchResult=By.xpath("//table[@class='x-grid-table x-grid-table-resizer']");
	public static By deleteAction=By.xpath("//tr/td[@title='Delete Action']");
	public static By editAction=By.xpath("//tr/td[@title='Edit Action']");
	public static By searchActionType=By.xpath("//table[not (@*)]//tr/td[2]");
	public static By searchActionName=By.xpath("//td[@class=' x-grid-cell x-grid-cell-name   ']");
	
